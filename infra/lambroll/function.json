{
  "FunctionName": "fit-energy-scraper",
  "Description": "Scrapes FIT portal for Excel files and uploads to S3",
  "Role": "arn:aws:iam::ACCOUNT_ID:role/fit-energy-scraper-role",
  "Code": {
    "ImageUri": "ACCOUNT_ID.dkr.ecr.REGION.amazonaws.com/fit-energy-scraper:latest"
  },
  "PackageType": "Image",
  "Timeout": 300,
  "MemorySize": 512,
  "Environment": {
    "Variables": {
      "S3_BUCKET": "fit-energy-data",
      "AWS_REGION": "us-east-1"
    }
  },
  "ReservedConcurrencyLimit": 1,
  "DeadLetterConfig": {
    "TargetArn": "arn:aws:sqs:REGION:ACCOUNT_ID:fit-energy-scraper-dlq"
  },
  "TracingConfig": {
    "Mode": "Active"
  },
  "LoggingConfig": {
    "LogFormat": "JSON",
    "LogGroup": "/aws/lambda/fit-energy-scraper"
  }
}